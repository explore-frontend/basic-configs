{
  "name": "react-admin",
  "version": "0.0.1",
  "type": "module",
  "packageManager": "pnpm@8.11.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:sourcemap": "vite build --sourcemap true",
    "serve": "vite preview",
    "lint": "eslint --ext .tsx,.ts,.js src",
    "lint:fix": "eslint --ext .tsx,.ts,.js src --fix",
    "format-check": "prettier --check .",
    "format-fix": "prettier --write .",
    "lint-staged": "lint-staged --allow-empty",
    "type-check": "tsc --noEmit",
    "ci-check": "concurrently --kill-others-on-fail \"pnpm lint\" \"pnpm type-check\" \"pnpm format-check\" \"pnpm build\" ",
    "commit": "git-cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "antd": "^5.6.4",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "immer": "^10.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.1",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@kwai-explore/eslint-config": "workspace:*",
    "@kwai-explore/tsconfig": "workspace:*",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "commitizen": "^4.3.0",
    "commitlint-config-cz": "^0.13.3",
    "concurrently": "^8.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.44.0",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.24",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
